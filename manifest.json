{
  "manifest_version": 2,
  "name": "Stuff Your Opinion",
  "version": "0.1.3",
  "description": "Hides opinions, reviews, and sponsored content on stuff.co.nz.",
  "permissions": [
    "storage"
  ],
  "icons": {
    "256": "icons/icon-256.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.stuff.co.nz/*"
      ],
      "js": [
        "stuff-your-opinion.js"
      ]
    }
  ]
}